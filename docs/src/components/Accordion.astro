---
import AccordionVue from "@/components/Accordion.vue";
import { Code } from "@astrojs/starlight/components";

interface Props {
   code: string;
   buttonText:string;
   fileName:string;
   lang: string;
}
const {code,buttonText, fileName, lang}=Astro.props
---
<AccordionVue client:load buttonText={buttonText} >
       <Code code={code} title={fileName} lang={lang} />
</AccordionVue>