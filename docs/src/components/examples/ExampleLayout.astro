---
import StarlightPage from '@astrojs/starlight/components/StarlightPage.astro';
const {pathname}= Astro.url
const basicHref='/example/vertical-list/single-vertical-list/'
const singleHorinzontal='/example/horizontal-list/single-horizontal-list/'
---

<StarlightPage   
  sidebar={[
      { label: 'VueFluidDnD', href: '/' },
      {
        label: 'Single vertical list',
        items: [
          { label: 'basic', href: basicHref,  isCurrent: basicHref===pathname },
        ],
      },
      {
        label: 'Single horizontal list',
        items: [
          { label: 'basic', href: singleHorinzontal,  isCurrent: singleHorinzontal===pathname },
        ],
      },
    ]} 
  frontmatter={{ title: 'Vertical list example of pokemons', description:'Sorting a list of pokemons with vue-fluid-dnd' ,tableOfContents:false }}  >
  <slot/>
</StarlightPage>
