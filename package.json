{
  "name": "vue-fluid-dnd",
  "version": "0.6.2",
  "description": "A Vue 3 drag and drop library to sort all kind of lists",
  "type": "module",
  "homepage": "https://vue-fluid-dnd.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/carlosjorger/vue-fluid-dnd.git"
  },
  "files": [
    "dist",
    "src/components/"
  ],
  "main": "./dist/vue-fluid-dnd.umd.cjs",
  "module": "./dist/vue-fluid-dnd.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-fluid-dnd.js",
      "require": "./dist/vue-fluid-dnd.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "dev": "cd my-test-examples && vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "vitest"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "dnd",
    "library",
    "drag",
    "drop",
    "drag&drop",
    "draggable",
    "droppable",
    "sortable",
    "lightweight",
    "dependency-free",
    "vue-fluid-dnd"
  ],
  "author": {
    "name": "Carlos Jorge Rodriguez",
    "email": "rodriguezcuelloc@gmail.com"
  },
  "peerDependencies": {
    "vue": ">=3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.0",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "jsdom": "^24.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.25"
  }
}
